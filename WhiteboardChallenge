public void DivisibleNumber(int number) 

{
    if(number % 3 == 0 && number % 5 ==0) 
    {
        Console.WriteLine("fizzbuzz");
    }

    else if(number % 3 == 0)
    {
        Console.WriteLine("fizz");
    }

    else if(number % 5 == 0)
    {
        Console.WriteLine("buzz");
    }
}

DivisibleNumber(45);